{
  "private": true,
  "dependencies": {
    "@loadable/component": "^5.10.3",
    "@material-ui/core": "^4.4.2",
    "@material-ui/icons": "^4.4.1",
    "axios": "^0.19.0",
    "firebase": "^7.0.0",
    "konva": "^3.4.1",
    "lodash": "^4.17.15",
    "material-table": "^1.50.0",
    "material-ui-popup-state": "^1.4.1",
    "mui-datatables": "^2.11.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-konva": "^16.8.7-3",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.0.1",
    "react-toastify": "^5.3.2",
    "redux": "^4.0.1",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "redux-undo": "file:third-party/redux-undo",
    "styled-components": "^5.0.0-beta.6",
    "uuid": "^3.3.3"
  },
  "scripts": {
    "preinstall": "./third-party/install.sh",
    "start": "cross-env NODE_ENV=development react-app-rewired start",
    "build": "cross-env NODE_ENV=production GENERATE_SOURCEMAP=false react-app-rewired build",
    "test": " react-app-rewired test",
    "eject": "echo \"Eject is not allowed\" && exit 1"
  },
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "sort-destructure-keys",
      "sort-exports"
    ],
    "rules": {
      "sort-exports/sort-exports": [
        "error",
        {
          "sortDir": "asc",
          "ignoreCase": false
        }
      ],
      "sort-destructure-keys/sort-destructure-keys": [
        "error",
        {
          "caseSensitive": true
        }
      ],
      "sort-keys": "error",
      "import/group-exports": "error",
      "react/jsx-sort-props": [
        "error",
        {
          "callbacksLast": true,
          "shorthandFirst": true,
          "shorthandLast": false,
          "ignoreCase": false,
          "noSortAlphabetically": false,
          "reservedFirst": true
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cross-env": "^6.0.0",
    "eslint-plugin-sort-destructure-keys": "^1.3.3",
    "eslint-plugin-sort-exports": "^0.1.1",
    "react-app-rewired": "^2.1.3"
  }
}
