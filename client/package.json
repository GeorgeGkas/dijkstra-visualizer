{
  "private": true,
  "dependencies": {
    "@loadable/component": "^5.12.0",
    "@material-ui/core": "^4.8.3",
    "@material-ui/icons": "^4.5.1",
    "axios": "^0.19.1",
    "firebase": "^7.6.2",
    "halogenium": "^2.3.0",
    "i18next": "^19.0.3",
    "i18next-xhr-backend": "^3.2.2",
    "konva": "^4.1.2",
    "lodash": "^4.17.19",
    "material-ui-popup-state": "^1.5.1",
    "mui-datatables": "^2.13.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-i18next": "^11.2.7",
    "react-konva": "^16.12.0-0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.3.0",
    "react-toastify": "^5.5.0",
    "reactour": "^1.16.0",
    "redux": "^4.0.5",
    "redux-axios-middleware": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "redux-undo": "file:third-party/redux-undo",
    "styled-components": "^4.4.1",
    "uuid": "^3.3.3"
  },
  "scripts": {
    "analyze": "cross-env NODE_ENV=production react-app-rewired build && source-map-explorer 'build/static/js/*.js'",
    "preinstall": "./third-party/install.sh",
    "start": "cross-env NODE_ENV=development react-app-rewired start",
    "build": "cross-env NODE_ENV=production GENERATE_SOURCEMAP=false react-app-rewired build",
    "test": " react-app-rewired test",
    "eject": "echo \"Eject is not allowed\" && exit 1"
  },
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "sort-destructure-keys",
      "sort-exports"
    ],
    "rules": {
      "sort-exports/sort-exports": [
        "error",
        {
          "sortDir": "asc",
          "ignoreCase": false
        }
      ],
      "sort-destructure-keys/sort-destructure-keys": [
        "error",
        {
          "caseSensitive": true
        }
      ],
      "sort-keys": "error",
      "import/group-exports": "error",
      "react/jsx-sort-props": [
        "error",
        {
          "callbacksLast": true,
          "shorthandFirst": true,
          "shorthandLast": false,
          "ignoreCase": false,
          "noSortAlphabetically": false,
          "reservedFirst": true
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "babel-jest": "^24.9.0",
    "cross-env": "^6.0.3",
    "eslint-plugin-sort-destructure-keys": "^1.3.3",
    "eslint-plugin-sort-exports": "^0.2.0",
    "react-app-rewired": "^2.1.5",
    "source-map-explorer": "^2.2.2"
  }
}
